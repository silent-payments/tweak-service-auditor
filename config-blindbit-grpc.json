{
  "services": [
    {
      "name": "blindbit-grpc",
      "service_type": "grpc",
      "endpoint": "127.0.0.1:50051",
      "active": true,
      "timeout": 10,
      "requests_per_second": 150,
      "dust_limit": 1
    },
    {
      "name": "blindbit-grpc-spent",
      "service_type": "grpc",
      "endpoint": "127.0.0.1:50051",
      "active": true,
      "timeout": 10,
      "requests_per_second": 150,
      "dust_limit": 1,
      "filter_spent": true
    },
    {
      "name": "blindbit-spent",
      "service_type": "http",
      "endpoint": "http://127.0.0.1:8000/tweaks",
      "headers": {
        "User-Agent": "TweakServiceAuditor/1.0"
      },
      "active": true,
      "requests_per_second": 100,
      "dust_limit": 0
    },
    {
      "name": "blindbit",
      "service_type": "http",
      "endpoint": "http://127.0.0.1:8000/tweak-index",
      "headers": {
        "User-Agent": "TweakServiceAuditor/1.0"
      },
      "active": true,
      "requests_per_second": 100,
      "dust_limit": 0
    }

  ],
  "service_pairs": [
    {
      "name": "grpc_vs_http",
      "service1": "blindbit-grpc",
      "service2": "blindbit",
      "active": true
    }
  ]
}